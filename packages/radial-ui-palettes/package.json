{
	"name": "radial-ui-palette",
	"version": "0.0.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"type": "module",
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --external fs,tinycolor2",
		"dev": "tsup src/index.ts --format esm,cjs --watch --dts",
		"lint": "eslint . --max-warnings 0",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"eslint": "^8.54.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2",
		"@types/tinycolor2": "^1.4.0",
		"@types/node": "20.10.0"
	},
	"publishConfig": {
		"access": "public"
	}
}